{
  "name": "solarsys",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc --noEmit && vite build",
    "build:ci": "npm ci && npm run build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "vitest --run",
    "deploy": "npm run build && wrangler pages deploy dist --project-name=solarsys",
    "deploy:preview": "npm run build && wrangler pages deploy dist --project-name=solarsys --compatibility-date=2024-01-01",
    "supabase:start": "npx supabase start",
    "supabase:stop": "npx supabase stop",
    "supabase:reset": "npx supabase db reset",
    "supabase:status": "npx supabase status",
    "supabase:studio": "npx supabase studio",
    "db:generate-types": "npx supabase gen types typescript --local > src/types/database.types.ts"
  },
  "dependencies": {
    "@cloudflare/kv-asset-handler": "^0.4.1",
    "@radix-ui/react-slot": "^1.2.4",
    "@supabase/supabase-js": "^2.38.5",
    "@types/uuid": "^10.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.0.0",
    "framer-motion": "^10.16.16",
    "lucide-react": "^0.562.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.48.2",
    "react-router-dom": "^6.20.1",
    "tailwind-merge": "^2.2.0",
    "tailwindcss-animate": "^1.0.7",
    "uuid": "^13.0.0",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@types/node": "^20.10.5",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "fast-check": "^3.15.0",
    "jsdom": "^27.3.0",
    "postcss": "^8.4.32",
    "supabase": "^2.67.2",
    "tailwindcss": "^3.4.0",
    "terser": "^5.44.1",
    "typescript": "^5.2.2",
    "vite": "^5.0.8",
    "vitest": "^1.1.0",
    "wrangler": "^4.56.0"
  }
}
